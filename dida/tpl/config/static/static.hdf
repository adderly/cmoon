index {
    Layout = layout.html
    Dataset {
        Layout {
            css = <link rel="stylesheet" type="text/css" href="/css/b/index.css" />
            js = <script src="/js/b/index.js"></script>
            title = 首页
            tabpart = 0
        }
        Include {
            content = index.html
        }
    }
    Output = index.html
}

icar {
    Layout = layout.html
    Dataset {
        Layout {
            css = <link rel="stylesheet" type="text/css" href="/css/b/icar.css" />
            js << EOM
                <script src="http://maps.google.com/maps/api/js?sensor=false&language=zh-CN&libraries=places"></script>
                <script type="text/javascript" src="/js/b/icar.js"></script>
EOM
            title = 邀车前往
#            botjs = <script src="http://maps.google.com/maps/api/js?sensor=false&language=zh-CN&libraries=places">
            tabpart = 1
        }
        Include {
            content = icar.html
        }
    }
    Output = icar.html
}
